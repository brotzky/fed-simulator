    "copy-webpack-plugin": "^4.0.0",
    "expect.js": "^0.3.1",
    "fastclick": "^1.0.6",
    "fbemitter": "^2.1.1",
    "glob": "^7.1.1",
    "hapi": "^16.0.2",
    "inert": "^4.0.4",
    "jquery": "^3.1.1",
    "lodash.maxby": "^4.6.0",
    "lodash.minby": "^4.6.0",
    "moment": "^2.17.1",
    "play-sound": "^1.1.1",
