{
  "name": "wwe-draft-generator",
  "version": "0.3.6",
  "repository": "https://github.com/azz0r/wwe-draft-generator.git",
  "scripts": {
    "start": "NODE_ENV=development DEBUG=app:* babel-node ./scripts/start.js",
    "build": "NODE_ENV=production DEBUG=app:* babel-node ./scripts/build.js",
    "build:deploy": "npm run build && npm run deploy",
    "deploy": "gh-pages-deploy",
    "eslint": "eslint src",
    "test": "NODE_ENV=test mocha --recursive -r mock-local-storage --compilers js:babel-core/register,js:./src/helpers/css-modules-compiler.js ./src/**/**/*.spec.js",
    "test:watch": "NODE_ENV=test mocha --watch --recursive -r mock-local-storage --compilers js:babel-core/register,js:./src/helpers/css-modules-compiler.js *.spec.js",
    "sprite": "webpack-dev-server --config config/webpack.config.sprite.js",
    "generate:ppvs": "babel-node ./scripts/generate/ppvs.js",
    "generate:wrestlers": "babel-node ./scripts/generate/wrestlers.js",
    "generate:brands": "babel-node ./scripts/generate/brands.js",
    "generate:championships": "babel-node ./scripts/generate/championships.js",
    "generate": "npm run generate:wrestlers && npm run generate:championships && npm run generate:brands && npm run generate:ppvs"
  },
  "devDependencies": {
    "asyncawait": "^1.0.6",
    "autoprefixer": "^6.5.1",
    "babel-eslint": "6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.18.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.2",
    "chalk": "^1.1.3",
    "cross-spawn": "^4.0.2",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.5",
    "debug": "^2.2.0",
    "emptyfunction": "^1.0.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.8.1",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-flowtype": "^2.23.0",
    "eslint-plugin-import": "1.12.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "1.0.1",
    "faker": "^3.1.0",
    "file-loader": "^0.9.0",
    "filesize": "3.3.0",
    "fs-extra": "0.30.0",
    "gh-pages-deploy": "^0.4.2",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "^2.24.0",
    "istanbul": "^0.4.5",
    "jsdom": "9.9.1",
    "jsdom-global": "2.1.1",
    "json-loader": "0.5.4",
    "jsx-control-statements": "^3.1.4",
    "mocha": "^3.1.2",
    "mock-local-storage": "^1.0.2",
    "node-sass": "^3.4.2",
    "opn": "4.0.2",
    "postcss-loader": "0.9.1",
    "react-addons-test-utils": "^15.3.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "2.5.4",
    "sass-loader": "^4.0.2",
